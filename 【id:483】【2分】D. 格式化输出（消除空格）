#include <iostream>
#include <cstring>
using namespace std;
int main()
{
    int t;
    cin>>t;
    cin.ignore();
    while(t--)
    {
        char c[1000];
        cin.getline(c,1000);
        int j = 0;
        for(int i=0;c[i]!='\0';i++)
        {
            c[j++] = c[i];
        }
        c[j] = '\0';
        int len = strlen(c);
        for(int i=0;i<len;i++)
        {
            if(c[i] == ' ')
            {
                for(int j=i; j<len; j++)
                {
                    c[j] = c[j+1];
                }
                len--;
                i--;
            }
        }
        cout<<c<<endl;
    }
    return 0;
}
