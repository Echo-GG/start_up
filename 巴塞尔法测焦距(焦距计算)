#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
double calculate(double x,double y)
{
	return (pow(x,2)-pow(y,2))/(4*x);
}
int main()
{
	double D[6];
	for(int i=0;i<6;i++)
	{
		cin>>D[i];
	}
	double d[6];
	for(int i=0;i<6;i++)
	{
		cin>>d[i];
	}
	for(int i=0;i<6;i++)
	{
		cout<<"f = "<<fixed<<setprecision(2)<<calculate(D[i],d[i])<<" cm"<<endl;
	}
	return 0;
}
